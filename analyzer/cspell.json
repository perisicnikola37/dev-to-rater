{
  "version": "0.2",
  "language": ["en"],
  "ignorePaths": [
    "node_modules",
    "dist",
    "build",
    "**/*.min.js",
    "**/*.min.css",
    "**/*.jpg",
    "**/*.jpeg",
    "**/*.png",
    "**/*.gif",
    "**/*.svg"
  ],
  "words": [
    "Nikola",
    "Perišić",
    "openxmlformats",
    "officedocument",
    "spreadsheetml",
    "sonner",
    "maxy",
    "canv",
    "maxx",
    "urlset",
    "lastmod",
    "gifsicle",
    "optipng",
    "pngquant",
    "senpai",
    "sensei"
  ],
  "cache": {
    "useCache": true,
    "cacheLocation": "./.cspell/.cspellcache",
    "cacheStrategy": "content"
  }
}
