{
  "version": "0.2",
  "language": ["en"],
  "ignorePaths": [
    "node_modules",
    "dist",
    "build",
    "**/*.min.js",
    "**/*.min.css",
    "**/*.jpg",
    "**/*.jpeg",
    "**/*.png",
    "**/*.gif",
    "**/*.svg"
  ],
  "words": [
    "Nikola",
    "Perišić",
    "openxmlformats",
    "officedocument",
    "spreadsheetml"
  ],
  "cache": {
    "useCache": true,
    "cacheLocation": "./.cspell/.cspellcache",
    "cacheStrategy": "content"
  }
}
